{
  "version": 2,
  "functions": {
      "api/index.py": {
          "runtime": "python3@3.9.12",  // Specify the exact version supported by Vercel
          "memory": 1024,
          "maxDuration": 30
      }
  },
  "routes": [
      {
      "src": "/(.*)",
      "dest": "/api/index.py"
      }
  ]
}
